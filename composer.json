{
    "name": "longhornhp/wave-stripe-php",
    "description": "Import Stripe bank deposits into Wave",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Abernathy",
            "email": "daniel@danielabernathy.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "laravel-zero/framework": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["application"]
}
